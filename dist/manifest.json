{
  "name": "Kendraio Verify",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Verify stuff on websites",
  "homepage_url": "https://kendra.io/",
  "browser_action": {
    "default_title": "Kendraio Verify",
    "default_popup": "action.html"
  },
  "background": {
    "scripts": [
      "js/vendor.js",
      "js/main.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icons/kendraio-logo.png"
  },
  "permissions": [
    "identity",
    "notifications"
  ]
}
